{
	"name": "@svelement-ui/col",
	"version": "0.9.9",
	"files": [
		"dist"
	],
	"exports": {
		".": {
			"import": "./src/index.svelte",
			"require": "./src/index.svelte",
			"svelte": "./src/index.svelte"
		},
		"./dist/style.css": {
			"import": "./dist/style.css",
			"require": "./dist/style.css"
		}
	},
	"types": "./dist/index.svelte.d.ts",
	"scripts": {
		"build": "concurrently \"pnpm:build-*\" && cp dist_style/style.css dist/style.css && rm -rf dist/style.css dist/style.d.ts dist_style/ && publint",
		"build-svelte": "svelte-kit sync && svelte-package -i src/",
		"build-css": "vite build --outDir dist_style/",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"clean": "rimraf dist"
	},
	"devDependencies": {
		"@svelement-ui/theme-chalk": "workspace:^",
		"@svelement-ui/util-array-2-class-string": "workspace:^",
		"@svelement-ui/util-array-2-style-string": "workspace:^"
	},
	"bundledDependencies": [
		"@svelement-ui/util-array-2-class-string",
		"@svelement-ui/util-array-2-style-string"
	],
	"publishConfig": {
		"access": "public",
		"registry": "https://registry.npmjs.org"
	}
}
