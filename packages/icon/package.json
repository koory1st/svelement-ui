{
  "name": "@svelement-ui/icon",
  "version": "0.10.2",
  "files": [
    "dist",
    "!dist/**/*.test.*",
    "!dist/**/*.spec.*"
  ],
  "svelte": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "svelte": "./dist/index.js"
    },
    "./dist/style.css": {
      "import": "./dist/style.css",
      "require": "./dist/style.css"
    }
  },
  "scripts": {
    "dev": "vite dev",
    "build": "vite build && concurrently \"pnpm:build-*\" && publint && cp dist_style/style.css dist/style.css && rm -rf dist/style.js dist/style.d.ts dist_style/",
    "build-svelte": "svelte-kit sync && svelte-package",
    "build-css": "vite build --outDir dist_style/ -c vite-style.config.ts",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "clean": "rimraf dist"
  },
  "peerDependencies": {
    "svelte": "^4.0.0"
  },
  "devDependencies": {
    "@svelement-ui/theme-chalk": "workspace:^",
    "@svelement-ui/util-array-2-class-string": "workspace:^",
    "@svelement-ui/util-array-2-style-string": "workspace:^"
  },
  "bundledDependencies": [
    "@svelement-ui/util-array-2-class-string",
    "@svelement-ui/util-array-2-style-string"
  ],
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org"
  }
}
