{
	"name": "@svelement-ui/aside",
	"version": "0.9.5",
	"files": [
		"dist"
	],
	"exports": {
		".": {
			"import": "./src/index.svelte",
			"require": "./src/index.svelte",
			"svelte": "./src/index.svelte"
		},
		"./dist/style.css": {
			"import": "./dist/style.css",
			"require": "./dist/style.css"
		}
	},
	"types": "./dist/index.svelte.d.ts",
	"scripts": {
		"build": "concurrently \"pnpm:build-*\" && cp dist_style/style.css dist/style.css && rm -rf dist/style.css dist/style.d.ts dist_style/",
		"build-svelte": "svelte-package -i src/",
		"build-css": "vite build --outDir dist_style/",
		"clean": "rimraf build dist"
	},
	"devDependencies": {
		"@svelement-ui/theme-chalk": "workspace:^",
		"sass": "^1.64.2"
	},
	"bundledDependencies": [
		"@svelement-ui/util-array-2-class-string"
	]
}
